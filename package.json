{
  "name": "acquired",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "rm -rf build && npm run build:client && npm run build:server",
    "build:client": "NODE_ENV=production next build",
    "build:server": "tsc --project tsconfig.server.json",
    "dev": "NODE_ENV=development ts-node-dev --respawn --transpile-only --project tsconfig.server.json server/index.ts",
    "start": "NODE_ENV=production node build/server/index.js"
  },
  "dependencies": {
    "@koa/cors": "^4.0.0",
    "@nextui-org/react": "^1.0.0-beta.9",
    "chokidar": "^3.5.3",
    "classnames": "^2.3.2",
    "config": "^3.3.8",
    "ffmpeg": "^0.0.4",
    "ffprobe": "^1.1.2",
    "inversify": "^6.0.1",
    "inversify-koa-utils": "^1.0.0",
    "koa-body": "^6.0.1",
    "koa-cookie": "^1.0.0",
    "koa-graceful-shutdown": "^1.2.1",
    "koa-router": "^12.0.0",
    "next": "12.1.6",
    "path": "^0.12.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-jss": "^10.9.2",
    "socket.io": "^4.5.4",
    "socket.io-client": "^4.5.4",
    "superagent": "^8.0.5",
    "uninstall": "^0.0.0"
  },
  "devDependencies": {
    "@types/chokidar": "^2.1.3",
    "@types/config": "0.0.41",
    "@types/ffmpeg": "^1.0.4",
    "@types/ffprobe": "^1.1.3",
    "@types/koa-cookie": "^1.0.0",
    "@types/koa-router": "^7.4.4",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@types/socket.io": "^3.0.2",
    "@types/superagent": "^4.1.16",
    "eslint": "8.18.0",
    "eslint-config-next": "12.1.6",
    "ffprobe-static": "^3.1.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "4.7.4"
  }
}
